               <p xml:space="preserve"><w>Venit</w> <w>autem</w> <w>quidam</w> <w>mulier</w> <w>nomine</w> <w>Engilrat</w> <w>ad</w> <w>Frigi­singa</w> <w>et</w> <w>tradidit</w> <w>ad</w> <w>domum</w> <w>electe</w> <w>dei</w> <w>genetricis</w> <w>Mariae</w> <seg function="Pertinenzformel"><w>mancipias</w> <w>VI</w> <w>proprietatis</w> <w>suae</w> <w>quorum</w> <w>nomina</w> <w>haec</w> <w>sunt</w>: <w>Hunrih</w>, <w>Tehuuart</w>, <w>Engilrih</w>, <w>Cozni</w>, <w>Liuposta</w>, <w>Adalni</w>, <seg function="Stipulationsformel"><w>quatenus</w> <w>ipsa</w> <w>mancipia</w> <w>sine</w> <w>ulla</w> <w>mutabilitate</w> <w>firmiter</w> <w>ad</w> <w>iam</w> <w>nominato</w> <w>dominato</w> <w>stabiles</w> <w>permanerent</w></seg> </seg>. <seg function="Corroboratio"><w>Et</w> <w>haec</w> <w>sunt</w> <w>testes</w></seg> : <w>Ala­uuih</w>. <w>Haholf</w>. <w>Cundhart</w>. <w>Petto</w>. <w>Cotafrid</w>. <w>Otperht</w>. <w>Arperht</w>. <seg function="Datierung"><w>Actum</w> <w>est</w> <w>hoc</w> <w>in</w> <w>VI</w>. <w>kal</w>. <w>ian</w></seg>. <w>anno</w> <w>Hloduuuici</w> <w>impera­toris</w> <w>VIII</w>. <w>indictione</w> <w>XV</w>.</p>
            </div>
