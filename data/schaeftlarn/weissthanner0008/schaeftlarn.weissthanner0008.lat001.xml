                     <p xml:space="preserve"><w>Ego</w> <w>Odalker</w> <w>tradidi</w> <w>atque</w> <w>confirmavi</w> <w>omnia</w>, <w>quae</w> <w>habui</w> <w>in</w> <w>loco</w> <w>nuncupante</w> <w>Walchstete</w>, <w>ad</w> <w>aecclesiam</w> <w>sancti</w> <w>Dionisii</w>, <w>quae</w> <w>constructa</w> <w>est</w> <w>in</w> <w>loco</w> <w>nuncupante</w> <w>Peipinpach</w>: <w>unum</w> <w>servum</w> <w>nomine</w> <w>Sigimunt</w> <w>et</w> <w>uxorem</w> <w>suam</w> <w>nomine</w> <w>Richswind</w> <w>cum</w> <w>casa</w>, <w>cum</w> <w>terra</w>, <w>cum</w> <w>campis</w>, <w>silvis</w>, <w>pratis</w>, <w>pomariis</w>, <w>omnia</w> <w>ex</w> <w>omnibus</w> <w>ad</w> <w>integrum</w>. <w>Isti</w> <w>sunt</w> <w>testes</w>: <w>Adalkart</w>, <w>Albrich</w>, <w>Orthere</w>, <w>Lantperth</w>, <w>Helfrich</w>, <w>Hunrich</w>, <w>Rantwik</w>, <w>Liuther</w>.</p>
